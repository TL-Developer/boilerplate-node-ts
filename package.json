{
  "name": "smart-schools-api-ts",
  "version": "1.0.0",
  "description": "Api with typescript",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "lint": "eslint ./src --ext .ts",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "start": "node ./dist/server",
    "dev": "ts-node-dev --inspect=0.0.0.0:9229 --respawn --transpile-only --ignore-watch node_modules ./src/server.ts",
    "build": "tsc",
    "style:check": "prettier --check src/**/*.ts tests/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts tests/**/*.ts",
    "test": "jest --testTimeout=10000",
    "test:watch": "jest --watch",
    "test:watch:coverage": "jest --watch --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/TL-Developer/smart-schools-api-ts.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/TL-Developer/smart-schools-api-ts/issues"
  },
  "homepage": "https://gitlab.com/TL-Developer/smart-schools-api-ts#readme",
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.13",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.6.3",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.0.1",
    "@typescript-eslint/parser": "^4.0.1",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.0",
    "husky": "^4.2.5",
    "i": "^0.3.6",
    "jest": "^26.4.2",
    "npm": "^6.14.8",
    "prettier": "^2.1.1",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "@overnightjs/logger": "^1.2.0",
    "chai": "^4.2.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2",
    "mongoose": "^5.11.8",
    "supertest": "^4.0.2"
  }
